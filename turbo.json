{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "outputs": [".next/**", "!.next/cache/**", "dist/**"]
    },
    "@aave/react-sdk#build": {
      "cache": false
    },
    "dev": {
      "dependsOn": ["@aave/react-sdk#build"]
    },
    "lint": {
      "cache": false
    },
    "test": {
      "cache": false
    },
    "test:coverage": {
        "cache": false
      }
  }
}
