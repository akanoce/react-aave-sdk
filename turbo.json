{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": [".next/**", "!.next/cache/**", "dist/**"]
    },
    "@aave/react-sdk#build": {
      "cache": false
    },
    "dev": {
      "dependsOn": ["@aave/react-sdk#build"]
    },
    "lint": {
      "cache": false
    }
  }
}
