{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": [".next/**", "!.next/cache/**", "dist/**"]
    },
    "react-aave-v3#build": {
      "cache": false
    },
    "dev": {
      "dependsOn": ["react-aave-v3#build"]
    },
    "lint": {
      "cache": false
    }
  }
}
